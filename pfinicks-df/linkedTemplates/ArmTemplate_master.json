{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "pfinicks-df"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"AzureSqlDatabase1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabase1'"
		},
		"imdbprojectingest_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'imdbprojectingest'"
		},
		"pfinicks_test_linked_service_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'pfinicks_test_linked_service'"
		},
		"HttpServer1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://datasets.imdbws.com/"
		},
		"imdbprojectingest_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://pfinickscloudstorage.dfs.core.windows.net/"
		},
		"pfinicks_test_linked_service_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://pfinickscloudstorage.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "pfinicks-df_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"AzureSqlDatabase1_connectionString": {
						"value": "[parameters('AzureSqlDatabase1_connectionString')]"
					},
					"imdbprojectingest_accountKey": {
						"value": "[parameters('imdbprojectingest_accountKey')]"
					},
					"pfinicks_test_linked_service_accountKey": {
						"value": "[parameters('pfinicks_test_linked_service_accountKey')]"
					},
					"HttpServer1_properties_typeProperties_url": {
						"value": "[parameters('HttpServer1_properties_typeProperties_url')]"
					},
					"imdbprojectingest_properties_typeProperties_url": {
						"value": "[parameters('imdbprojectingest_properties_typeProperties_url')]"
					},
					"pfinicks_test_linked_service_properties_typeProperties_url": {
						"value": "[parameters('pfinicks_test_linked_service_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}